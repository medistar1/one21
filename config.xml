<?xml version='1.0' encoding='utf-8'?>
<widget id="com.win.rol" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>rol</name>
    <description>
        A ball software.
    </description>
    <author email="winroulettespinsoftware@gmail.com" href="http://winroulettespin.info">
        WinRoulette Team
    </author>
    <content src="index.html" />
    <access origin="*" />
     <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-targetSdkVersion" value="24" />
    <preference name="orientation" value="landscape" />
    <preference name="prerendered-icon" value="true"/>
     <icon src="img/icon.png" gap:role="default" />
      <platform name="android">
        <allow-intent href="market:*" />
    </platform>
     <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
